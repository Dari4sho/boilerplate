{
  "name": "Z63Boilerplate",
  "preferGlobal": "false",
  "version": "1.7.0",
  "description": "Boilerplate of Z63",
  "devDependencies": {
    "browserify": "^11.0.1",
    "browserify-shim": "^3.8.7",
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^3.0.0",
    "grunt-browserify": "^4.0.0",
    "grunt-concurrent": "^2.0.0",
    "grunt-contrib-connect": "^0.11.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-sass": "^1.0.0",
    "grunt-scss-lint": "^0.3.6",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-connect": "^2.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-minify-css": "^1.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.0.4",
    "gulp-scss-lint": "^0.2.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.3.0",
    "jit-grunt": "^0.9.1",
    "jshint-stylish": "^2.0.1",
    "time-grunt": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "jquery": "2.1.4"
  },
  "browser": {
    "modernizr": "./source/js/vendor/modernizr.js"
  },
  "browserify-shim": {
    "modernizr": "Modernizr"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "jshintConfig": {
    "passfail": false,
    "maxerr": 100,
    "browser": true,
    "jquery": false,
    "debug": false,
    "devel": true,
    "es3": true,
    "strict": false,
    "globalstrict": true,
    "asi": false,
    "boss": false,
    "curly": true,
    "eqeqeq": true,
    "eqnull": false,
    "evil": false,
    "expr": false,
    "forin": false,
    "immed": true,
    "latedef": true,
    "loopfunc": true,
    "shadow": false,
    "supernew": false,
    "undef": true,
    "unused": true,
    "nomen": true,
    "onevar": false,
    "plusplus": false,
    "sub": true,
    "trailing": true,
    "white": false,
    "indent": 4,
    "quotmark": "single",
    "globals": {
      "require": true,
      "module": true
    }
  }
}
